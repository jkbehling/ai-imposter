{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Daphne: ASGI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "project.asgi:application",
                "--host",
                "0.0.0.0",
                "--port",
                "8000",
                "--reload"
            ],
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal"
        }
    ]
}